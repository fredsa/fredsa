<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">

  <servlet>
    <servlet-name>Tq</servlet-name>
    <servlet-class>com.google.tq.TqServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Tq</servlet-name>
    <url-pattern>/tq/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Query</servlet-name>
    <servlet-class>com.google.tq.QueryServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Query</servlet-name>
    <url-pattern>/query/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Delete</servlet-name>
    <servlet-class>com.google.tq.DeleteServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Delete</servlet-name>
    <url-pattern>/delete/*</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>tq</welcome-file>
  </welcome-file-list>
</web-app>
