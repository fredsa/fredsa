<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .grid {
    }
    @export numColumn;
    @export wordColumn;
    .numColumn {
      width: 30px;
    }
    .wordColumn {
      width: 300px;
    }
  </ui:style>
  <g:HTMLPanel>
    <g:FlexTable styleName="{style.grid}" ui:field="grid"></g:FlexTable>
  </g:HTMLPanel>
</ui:UiBinder> 