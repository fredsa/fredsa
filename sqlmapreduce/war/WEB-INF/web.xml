<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <servlet>
    <servlet-name>rpcServlet</servlet-name>
    <servlet-class>sqlmapreduce.server.RpcServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>rpcServlet</servlet-name>
    <url-pattern>/sqlmapreduce/rpc</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>mapreduce</servlet-name>
    <servlet-class>com.google.appengine.tools.mapreduce.MapReduceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mapreduce</servlet-name>
    <url-pattern>/mapreduce/*</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Sqlmapreduce.html</welcome-file>
  </welcome-file-list>

</web-app>
