  <ui:style>
    .panel {
      border: 3px solid black;
      padding: 5px;
    }
  </ui:style>
  <g:HTMLPanel>
    <g:SimplePanel styleName="{style.panel}" ui:field="challengePanel">
    </g:SimplePanel>
    <g:SimplePanel styleName="{style.panel}" ui:field="wordListPanel">
    </g:SimplePanel>
    <g:SimplePanel styleName="{style.panel}" ui:field="loggerPanel">
    </g:SimplePanel>
  </g:HTMLPanel>
</ui:UiBinder> 